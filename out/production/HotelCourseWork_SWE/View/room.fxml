            <Cursor fx:constant="OPEN_HAND" />
         </cursor>
         <font>
            <Font name="Bell MT Bold" size="31.0" />
         </font>
      </Button>
      <Button layoutX="317.0" layoutY="239.0" mnemonicParsing="false" onAction="#deleteOnAction" opacity="0.0" prefHeight="74.0" prefWidth="296.0" text="Delete Room">
