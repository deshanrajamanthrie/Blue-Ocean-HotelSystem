<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.RoomController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="907.0" layoutX="-4.0" pickOnBounds="true">
         <image>
            <Image url="@../asserts/4.png" />
         </image>
      </ImageView>
      <Button layoutX="317.0" layoutY="133.0" mnemonicParsing="false" onAction="#addroomOnAction" opacity="0.0" prefHeight="74.0" prefWidth="296.0" text="Add New Room" textFill="#462626">
